{
  "$schema": "https://vim-fall.github.io/jsonschema/v1alpha/package.schema.json",
  "action": {
    "quickfix": {
      "url": "./action/quickfix.ts",
      "variants": {
        "add": {
          "action": "a"
        },
        "replace": {
          "action": "r"
        },
        "processed": {
          "target": "selected-or-processed"
        },
        "processed:add": {
          "target": "selected-or-processed",
          "action": "a"
        },
        "processed:replace": {
          "target": "selected-or-processed",
          "action": "r"
        }
      }
    }
  },
  "sorter": {
    "lexical": {
      "url": "./sorter/lexical.ts",
      "variants": {
        "reverse": {
          "reverse": true
        }
      }
    }
  },
  "source": {
    "line": {
      "url": "./source/line.ts"
    }
  }
}
