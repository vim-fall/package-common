{
  "$version": "1",
  "extension": {
    "action": {
      "quickfix": {
        "url": "./action/quickfix.ts",
        "variants": {
          "add": {
            "action": "a"
          },
          "replace": {
            "action": "r"
          },
          "processed": {
            "target": "selected-or-processed"
          },
          "processed:add": {
            "target": "selected-or-processed",
            "action": "a"
          },
          "processed:replace": {
            "target": "selected-or-processed",
            "action": "r"
          }
        }
      }
    },
    "processor": {
      "lexical_sorter": {
        "url": "./processor/lexical_sorter.ts",
        "variants": {
          "reverse": {
            "reverse": true
          }
        }
      }
    },
    "source": {
      "line": {
        "url": "./source/line.ts"
      }
    }
  }
}
